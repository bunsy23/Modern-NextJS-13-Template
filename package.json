{
  "name": "modern-next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pre-commit": "lint-staged",
    "postinstall": "husky install",
    "prisma:db:push": "dotenv -e .env.local -- npx prisma db push",
    "prisma:db:seed": "dotenv -e .env.local -- npx prisma db seed",
    "prisma:migrate:dev": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:studio": "dotenv -e .env.local -- npx prisma studio"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@radix-ui/primitive": "^1.0.0",
    "@radix-ui/react-avatar": "^1.0.2",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/node": "18.15.13",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "bcrypt": "^5.1.0",
    "class-variance-authority": "^0.5.3",
    "clsx": "^1.2.1",
    "commitizen": "^4.3.0",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "http-status-codes": "^2.2.0",
    "lucide-react": "^0.181.0",
    "next": "13.3.0",
    "next-auth": "^4.22.1",
    "nodemailer": "^6.9.1",
    "postcss": "8.4.23",
    "prettier": "^2.8.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-wrap-balancer": "^1.0.0",
    "tailwind-merge": "^1.12.0",
    "tailwindcss": "3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "concurrently": "^8.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-cli": "^7.2.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "prisma": "^4.13.0",
    "tailwindcss-animate": "^1.0.5",
    "tailwindcss-debug-screens": "^2.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
